package com.g4.beans;

public class Leaflet {

	private String id;
	private String content;

	private Leaflet(){
		
	}
	private Leaflet(String s){
		this.id = s;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}
	
}
